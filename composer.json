{
    "name": "ttskch/contact-form",
    "description": "PHP utility classes to implement general contact form",
    "keywords": ["contact-form", "form", "email", "sendmail"],
    "homepage": "https://github.com/ttskch/contact-form",
    "license": "MIT",
    "authors": [
        {
            "name": "Takashi Kanemoto",
            "email": "ttskch@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "swiftmailer/swiftmailer": ">=5.4.12"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8,<7.0"
    },
    "autoload": {
        "psr-4": {
            "Ttskch\\ContactForm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ttskch\\ContactForm\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    }
}
